---
import HeadContent from '../components/common/HeadContent.astro';
interface Props {
	title: string;
}

const {title} = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
        <HeadContent />

		<title>{title}</title>
		<slot name="head" />
	</head>
	<body>
		<slot />
	</body>
</html>
